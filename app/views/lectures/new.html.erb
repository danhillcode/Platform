<h1>Make a new lecture</h1>

<%= form_for [@teacher, @lecture], url:  teacher_lectures_path do |f| %>
  <%= f.label :topic %>:
  <%= f.text_field :topic %><br />

<%= f.label :time %>:
  <%= f.text_field :time %><br />

<%= f.label :hangoutId %>:
  <%= f.text_field :hangoutId %><br />

<%= f.label :studentAmount %>:
  <%= f.text_field :studentAmount %><br />

  <%= f.submit %>


<% end %>


<%= link_to "<button>Go back to your profile</button>".html_safe, user_session_path, :id => "button_id", :class => "button_class" %>

